<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
  <bean id="addressbean" class="com.springexample.springexample.Address">
   <property name="city" value = "pune"></property>
  </bean>
<bean id="studentbean" class="com.springexample.springexample.Student">  
<property name="rollNo" value="10"></property>  
<property name="firstName" value="10"></property>  
<property name="lastName" value="10"></property>  
<property name="department" value="10"></property>  
<property name="address" ref="addressbean"></property>

</bean>  
  
</beans>     